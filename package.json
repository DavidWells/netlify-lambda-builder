{
  "name": "netlify-functions-builder",
  "version": "1.0.0",
  "description": "A Netlify functions builder experiment ",
  "main": "index.js",
  "bin": "./bin/netlify-functions-builder",
  "scripts": {
    "test": "run-s test:*",
    "test:lint": "eslint .",
    "test:jest": "jest",
    "build": "run-s clean && run-p build:*",
    "build:site": "sitedown . -b build/ -l layout.html",
    "build:functions": "./bin/netlify-functions-builder",
    "clean": "rimraf build && mkdirp build && rimraf 'functions/*{.zip,/node_modules}'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bcomnes/netlify-lambda-builder.git"
  },
  "keywords": [
    "netlify",
    "lambda",
    "functions",
    "build"
  ],
  "author": "Bret Comnes",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bcomnes/netlify-lambda-builder/issues"
  },
  "homepage": "https://github.com/bcomnes/netlify-lambda-builder#readme",
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "eslint": "^4.15.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-prettier": "^2.1.2",
    "jest": "^23.1.0",
    "npm-run-all": "^4.0.2",
    "prettier": "^1.10.1",
    "sitedown": "^3.3.0"
  },
  "dependencies": {
    "archiver": "^2.1.1",
    "cliclopts": "^1.1.1",
    "glob": "^7.1.2",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "pump": "^3.0.0",
    "run-parallel-limit": "^1.0.5",
    "run-series": "^1.1.8"
  }
}
